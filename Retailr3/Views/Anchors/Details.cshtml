                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label>@Html.DisplayNameFor(model => model.LicenceNumber)</label>
                            <label readonly asp-for="LicenceNumber" class="form-control"></label>
                            @*<label class="form-control border-input">@Html.DisplayFor(model => model.RegionDescription)</label>*@
                        </div>
                    </div>
                </div>

